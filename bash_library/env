# ------------------------------------------------------------------------------
# env - add some environment variables for common directories
#
# Use both variables and aliases here:
# aliases for quick navigation, variables for other scripts
# ------------------------------------------------------------------------------
# ~/Work
export WORK="$HOME/Work"
export PROJECTS="$WORK/Projects"
export DOTFILES="$WORK/Projects/msteinhoff/dotfiles"
export SCRIPTS="$WORK/Projects/msteinhoff/scripts"

alias projects="cd $PROJECTS"
